<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="playground.css" />
		
	<ui:style field="icons" src="icons.css" />
		
	<ui:image field="entranceImage" src="entrance.icon.png"></ui:image>
	<ui:image field="archivesImage" src="archives.icon.png"></ui:image>
	<ui:image field="editionImage" src="edition.icon.png"></ui:image>
	<ui:image field="bookaImage" src="booka.icon.png"></ui:image>
	<ui:image field="accountImage" src="account.icon.png"></ui:image>
	<ui:image field="contactImage" src="contact.icon.png"></ui:image>

	<g:DockLayoutPanel unit='PX'>
		<g:north size='80'>
			<g:FlowPanel styleName="{style.navigation}">
				<g:Hyperlink styleName="{icons.icon} {icons.entrance}"
					targetHistoryToken="/entrance" ui:field="entrance" />
				<g:Hyperlink styleName="{icons.icon} {icons.archives}"
					targetHistoryToken="/docs" ui:field="archives" />
				<g:Hyperlink styleName="{icons.icon} {icons.edition}"
					targetHistoryToken="/edition" ui:field="edition" />
				<g:Hyperlink styleName="{icons.icon} {icons.booka}"
					targetHistoryToken="/booka" ui:field="booka" />
				<g:Hyperlink styleName="{icons.icon} {icons.right} {icons.account}"
					targetHistoryToken="/account" ui:field="account" />
				<g:Hyperlink styleName="{icons.icon} {icons.right} {icons.contact}"
					targetHistoryToken="/contact" ui:field="contact" />
			</g:FlowPanel>
		</g:north>
		<g:south size="16">
			<g:Label styleName="{style.status}" ui:field="status">This is the
				status</g:Label>
		</g:south>


		<g:west size="300">
			<g:LayoutPanel ui:field="explorer" styleName="{style.explorer}" />
		</g:west>

		<g:east size="250">
			<g:LayoutPanel ui:field="tools" styleName="{style.tools}" />
		</g:east>

		<g:center>
			<g:LayoutPanel ui:field="content" styleName="{style.content}" />
		</g:center>

	</g:DockLayoutPanel>
</ui:UiBinder> 