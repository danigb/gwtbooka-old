<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style field="ui" src="../Booka.css" />
	<ui:style src="BookaUI.css" />
		
	<ui:image field="entranceImage" src="img/entrance.icon.png"></ui:image>
	<ui:image field="archivesImage" src="img/archives.icon.png"></ui:image>
	<ui:image field="editionImage" src="img/edition.icon.png"></ui:image>
	<ui:image field="bookaImage" src="img/booka.icon.png"></ui:image>
	<ui:image field="accountImage" src="img/account.icon.png"></ui:image>
	<ui:image field="contactImage" src="img/contact.icon.png"></ui:image>
	<ui:image field="calendarImage" src="img/calendar.png"></ui:image>

	<g:DockLayoutPanel unit='PX' ui:field="self">
		<g:north size='80'>
			<g:FlowPanel styleName="{style.navigation}">
				<g:Hyperlink styleName="{style.icon} {style.entrance}"
					targetHistoryToken="/entrance" ui:field="entrance" />
				<g:Hyperlink styleName="{style.icon} {style.archives}"
					targetHistoryToken="/archives" ui:field="archives" />
				<g:Hyperlink styleName="{style.icon} {style.edition}"
					targetHistoryToken="/edition" ui:field="edition" />
				<g:Hyperlink styleName="{style.icon} {style.booka}"
					targetHistoryToken="/booka" ui:field="booka" />
				<g:Hyperlink styleName="{style.icon} {style.right} {style.calendar}"
					targetHistoryToken="/calendar" ui:field="calendar" />
				<g:Hyperlink styleName="{style.icon} {style.right} {style.account}"
					targetHistoryToken="/account" ui:field="account" />
				<g:Hyperlink styleName="{style.icon} {style.right} {style.contact}"
					targetHistoryToken="/contact" ui:field="contact" />
			</g:FlowPanel>
		</g:north>
				<g:south size="16">
			<g:Label styleName="{style.status}" ui:field="status">This is the
				status</g:Label>
		</g:south>
	</g:DockLayoutPanel>
</ui:UiBinder> 