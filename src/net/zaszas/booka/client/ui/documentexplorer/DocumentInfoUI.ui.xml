<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style src="DocumentInfoUI.css" />
		
	<g:HTMLPanel styleName="{style.docInfo}">
		<g:Hyperlink styleName="{style.title}" ui:field="title">Title</g:Hyperlink>
		<span ui:field="description" class="{style.description}">Description</span>
		<span ui:field="extra" class="{style.extra}">Extra information</span>
	</g:HTMLPanel>
</ui:UiBinder> 