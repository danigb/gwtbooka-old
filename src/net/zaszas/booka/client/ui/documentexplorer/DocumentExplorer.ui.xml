<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="DocumentExplorer.css" />
		
	<g:DockLayoutPanel unit="PX">
		<g:north size="30">
			<g:HTMLPanel stylePrimaryName="actions" styleName="{style.actions}">
				<g:Hyperlink ui:field="addProjectAction"
				targetHistoryToken="/docs/new" 
				styleName="{style.action}">Añadir artículo</g:Hyperlink>
				<g:Hyperlink ui:field="addDocumentAction"
				targetHistoryToken="/docs/new" 
				styleName="{style.action}">Añadir documento</g:Hyperlink>
			</g:HTMLPanel>
		</g:north>
		<g:center>
			<g:FlowPanel styleName="{style.list}" ui:field="list"></g:FlowPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 